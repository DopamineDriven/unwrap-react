{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["ESNext"],
    "module": "ESNext",
    "target": "ES6",
    "strict": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "noStrictGenericChecks": false,
    "allowJs": true,
    "declaration": true,
    "jsx": "react-jsx"
  }
}
